<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flagMakerConfig>
   <flagCfg>
        <dataName>wikipedia</dataName>
        <distributionArgs>none</distributionArgs>
        <extraIngestArgs></extraIngestArgs>
        <folder>wikipedia</folder>
        <inputFormat>nsa.datawave.ingest.wikipedia.WikipediaEventInputFormat</inputFormat>
        <!-- If we want to use bulkingest -->
        <!--ingestPool>onehr</ingestPool-->
        <ingestPool>fivemin</ingestPool>
    </flagCfg>
    <defaultCfg>
        <!-- If we want to use bulkingest --> 
        <!--maxFlags>${INGEST_MAX_ONE_HR_BLOCKS_PER_JOB}</maxFlags>
        <reducers>${ONE_HR_INGEST_REDUCERS}</reducers>
        <maxFlags>${INGEST_MAX_ONE_HR_BLOCKS_PER_JOB}</maxFlags>
        <script>bin/ingest/one-hr-ingest.sh</script-->

        <!-- currently only require a few of the params for the default config -->
        <reducers>${FIVE_MIN_INGEST_REDUCERS}</reducers>
        <maxFlags>${INGEST_MAX_FIVE_MIN_BLOCKS_PER_JOB}</maxFlags>
        <script>bin/ingest/five-min-ingest.sh</script>
    </defaultCfg>
    <baseHDFSDir>${HDFS_BASE_DIR}</baseHDFSDir>
    <distributorType>simple</distributorType>
    <!-- No dot "." files -->
    <filePattern>[0-9a-zA-Z]*</filePattern>
    <hdfs>${INGEST_HDFS_NAME_NODE}</hdfs>
    <socketPort>20000</socketPort>
    <datawaveHome>${DATAWAVE_INGEST_HOME}</datawaveHome>
    <flagFileDirectory>${FLAG_DIR}</flagFileDirectory>
    <setFlagFileTimestamp>false</setFlagFileTimestamp>
    <useFolderTimestamp>false</useFolderTimestamp>
</flagMakerConfig>
