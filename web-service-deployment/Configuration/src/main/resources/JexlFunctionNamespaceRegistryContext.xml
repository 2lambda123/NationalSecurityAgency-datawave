<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:util="http://www.springframework.org/schema/util"
    xsi:schemaLocation="
     http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
     http://www.springframework.org/schema/util
     http://www.springframework.org/schema/util/spring-util-4.0.xsd">

    <!-- Query Function definitions -->
    <util:map id="jexlEngineFunctionMap" key-type="java.lang.String" value-type="java.lang.String">
        <entry key="f" value="nsa.datawave.query.rewrite.jexl.functions.QueryFunctions"/>
        <entry key="geo" value="nsa.datawave.query.rewrite.jexl.functions.GeoFunctions"/>
        <entry key="geowave" value="nsa.datawave.query.rewrite.jexl.functions.GeoWaveFunctions"/>
        <entry key="content" value="nsa.datawave.query.rewrite.jexl.functions.ContentFunctions"/>
        <entry key="normalize" value="nsa.datawave.query.rewrite.jexl.functions.NormalizationFunctions"/>
        <entry key="filter" value="nsa.datawave.query.rewrite.jexl.functions.EvaluationPhaseFilterFunctions"/>
        <entry key="grouping" value="nsa.datawave.query.rewrite.jexl.functions.GroupingRequiredFilterFunctions"/>
    </util:map>
    
</beans>
