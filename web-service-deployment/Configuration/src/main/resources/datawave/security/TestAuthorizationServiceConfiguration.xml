<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
                           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.1.xsd">
<!--
     Define one entry section per entry you want in the test verifier.  They key is
     the DN, and it must appear in lower case, following the example below (the order
     of the DN components must match).  The value should contain a list of Auths
     exactly as they would be returned by Accumulo.
     
     <entry key="cn=test a. user, ou=my department, o=my company, st=some-state, c=us">
              <list>
 				<value>AuthorizedUser</value>
              </list>
      </entry>
     
     -->
    <util:map id="testAuthorizationServiceRoles" key-type="java.lang.String" value-type="java.util.List" map-class="java.util.HashMap">
             ${security.testauthservice.entries}
    </util:map>

</beans>
